{\pard \ql \f0 \sa180 \li0 \fi0 \outlinelevel2 \b \fs28 ffmpeg build configuration\par}
{\pard \ql \f0 \sa180 \li0 \fi0 \f1 mkdir -p build\line
cd build\line
../configure \\\line
  --prefix=$PWD/install-root \\\line
  --arch=x86_64 \\\line
  --disable-debug \\\line
  --disable-static \\\line
  --disable-doc \\\line
  --disable-htmlpages \\\line
  --disable-manpages \\\line
  --disable-podpages \\\line
  --disable-txtpages \\\line
  --disable-bzlib \\\line
  --disable-iconv \\\line
  --disable-lzo \\\line
  --disable-network \\\line
  --disable-schannel \\\line
  --disable-symver \\\line
  --disable-xlib \\\line
  --disable-zlib \\\line
  --disable-lzma \\\line
  --disable-sdl2 \\\line
  --disable-securetransport \\\line
  --disable-avdevice \\\line
  --disable-avfilter \\\line
  --disable-postproc \\\line
  --disable-demuxers \\\line
  --disable-muxers \\\line
  --disable-decoders \\\line
  --disable-encoders \\\line
  --disable-bsfs \\\line
  --disable-parsers \\\line
  --disable-hwaccels \\\line
  --disable-filters \\\line
  --disable-devices \\\line
  --disable-protocols \\\line
  --enable-shared \\\line
  --enable-rpath \\\line
  --enable-small \\\line
  --enable-swresample \\\line
  --enable-swscale \\\line
  --enable-demuxer=avi,matroska \\\line
  --enable-muxer=avi,matroska \\\line
  --enable-decoder=pcm_s16le,rawvideo,utvideo \\\line
  --enable-encoder=pcm_s16le,rawvideo,utvideo \\\line
  --enable-protocol=file\line
\line
make install\line
if [[ "$OSTYPE" == "linux-gnu"* ]]; then\line
  cd $PWD/install-root/lib\line
  mv $(readlink libavcodec.so) libavcodec.so\line
  mv $(readlink libavformat.so) libavformat.so\line
  mv $(readlink libavutil.so) libavutil.so\line
  mv $(readlink libswresample.so) libswresample.so\line
  mv $(readlink libswscale.so) libswscale.so\line
  rm -f libavcodec.so.* libavformat.so.* libavutil.so.* libswresample.so.* libswscale.so.*\line
fi\par}
